.IX Item "-c, --connect=dev"
.Vb 1
\&  connect I<filename> to NBD device I<dev>
.Ve
.IP "\fB\-d, \-\-disconnect\fR" 4
.IX Item "-d, --disconnect"
.Vb 1
\&  disconnect the specified device
.Ve
